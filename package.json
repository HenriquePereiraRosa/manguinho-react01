    "rimraf": "^5.0.1"
  },
  "devDependencies": {
    "@faker-js/faker": "6.1.2",
    "@testing-library/react": "^14.0.0",
    "@types/faker": "^6.6.9",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.2.5",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
