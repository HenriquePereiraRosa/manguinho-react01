  "license": "UNLICENCED",
  "main": "index.js",
  "scripts": {
    "prepare": "husky install",
    "clean": "rimraf dist",
    "test": "jest --coverage --no-cache --runInBand",
    "test:watch": "yarn test --watch",
    "test:staged": "yarn test --findRelatedTests",
    "lint": "eslint .",
    "lint:fix": "yarn lint --fix",
    "build": "webpack --config webpack.prod.js --stats-children",
    "dev": "concurrently \"webpack-dev-server --stats-error-details --config webpack.dev.js\" \"node server.js\"",
    "start": "webpack-dev-server --stats-error-details"
  },
